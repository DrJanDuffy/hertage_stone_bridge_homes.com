{
  "name": "heritage-stonebridge-website",
  "description": "Heritage at Stonebridge - Luxury 55+ Active Adult Community Website",
  "version": "1.0.0",
  "engines": {
    "node": ">=18.0.0",
    "pnpm": ">=8.0.0"
  },
  "private": true,
  "scripts": {
    "build": "qwik build --mode production",
    "build.client": "vite build",
    "build.preview": "vite build --ssr src/entry.preview.tsx",
    "build.server": "vite build -c adapters/vercel-edge/vite.config.ts",
    "build.types": "tsc --incremental --noEmit",
    "build.analyze": "vite build --mode analyze",
    "deploy": "vercel deploy",
    "deploy.prod": "vercel deploy --prod",
    "dev": "vite --mode ssr",
    "dev.debug": "node --inspect-brk ./node_modules/vite/bin/vite.js --mode ssr --force",
    "lint": "biome check src",
    "lint.fix": "biome check --write src",
    "format": "biome format --write src",
    "preview": "qwik build preview && vite preview --open",
    "start": "vite --open --mode ssr",
    "qwik": "qwik",
    "perf": "npm run build"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.2.4",
    "@builder.io/qwik": "1.0.0",
    "@builder.io/qwik-city": "~1.0.0",
    "@types/node": "^20.0.0",
    "autoprefixer": "^10.4.14",
    "postcss": "^8.4.23",
    "tailwindcss": "^3.3.1",
    "typescript": "5.0.4",
    "undici": "5.22.0",
    "vercel": "^28.19.0",
    "vite": "4.3.2",
    "vite-tsconfig-paths": "4.2.0",
    "terser": "^5.24.0"
  },
  "dependencies": {
    "@builder.io/qwik": "1.0.0",
    "@builder.io/qwik-city": "~1.0.0"
  },
  "keywords": [
    "real-estate",
    "55-plus-communities",
    "active-adult",
    "las-vegas",
    "summerlin",
    "heritage-stonebridge",
    "luxury-homes",
    "gated-community"
  ],
  "author": {
    "name": "Dr. Jan Duffy",
    "email": "DrDuffySells@HeritageStonebridge.com",
    "url": "https://heritagestonebridge.com"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/DrJanDuffy/hertage_stone_bridge_homes.com"
  },
  "homepage": "https://heritagestonebridge.com"
}
